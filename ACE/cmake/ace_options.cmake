
option(ACE_HAS_IPV6 "" ON)
option(ACE_INLINE "" ON)
option(ACE_HAS_ATM "" OFF)
option(ACE_HAS_THREADS "" ON)
option(ACE_USES_WCHAR "" OFF)

option(ACE_SVCCONF "" ON)
option(ACE_TOKEN "" ON)
