  set(GEN_OSTREAM_OPS ON)
endif()


set(ACE_OPTIONS ACE_INLINE ACE_HAS_IPV6 ACE_HAS_THREADS ACE_USES_WCHAR ACE_SVCCONF ACE_TOKEN ACE_OTHER ACE_HAS_VERSIONED_NAMESPACE GEN_OSTREAM)